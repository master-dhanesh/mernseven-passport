<% posts.forEach(function(p){ %>
<h2><%= p.title%></h2>
<p><%= p.description%></p>
<a href="/delete/<%= p._id%>">Delete</a>
<a href="/update/<%= p._id%>">Update</a>
<hr />
<% })%>

